<!Doctype html>

<html>
    <head>
        <% include ../partials/head %>
    </head>
    <body>
     
        <% include ../partials/navigations %>

        <h1><%= headline %></h1>

        <ul>
            <% tweets.forEach(function (tweet) { %>

                <li>
                    <img src="<%= tweet.user.avatar %>">
                    <% tweet.text %>
                    <p><%= tweet.user.name %></p>
                    <p>@<%= tweet.user.nick %></p>
                </li>

           <% }) %>
        </ul>
        
    </body>
</html>